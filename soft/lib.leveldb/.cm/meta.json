{
  "auto_detect": "yes",
  "customize": {
    "check_that_exists": "yes",
    "ck_version": 10,
    "env_prefix": "CK_ENV_LIB_LEVELDB",
    "file_root_name": "libleveldb",
    "limit_recursion_dir_search": {
      "linux": 4,
      "win": 4
    },
    "soft_file": {
      "linux": "libleveldb$#file_ext_dll#$"
    },
    "soft_path_example": {
      "linux": "/usr/local/lib/libleveldb.dylib"
    }
  },
  "deps": {},
  "soft_name": "LevelDB library",
  "tags": [
    "lib",
    "leveldb"
  ]
}
