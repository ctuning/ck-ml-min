{
  "auto_detect": "yes",
  "customize": {
    "check_that_exists": "yes",
    "ck_version": 10,
    "env_prefix": "CK_ENV_LIB_MPC",
    "include_file": "mpc.h",
    "limit_recursion_dir_search": {
      "linux": 3
    },
    "soft_file": {
      "linux": "libmpc.so"
    },
    "soft_path_example": {
      "linux": ""
    }
  },
  "deps": {
    "lib-gmp": {
      "local": "yes",
      "name": "GMP library",
      "sort": 10,
      "tags": "lib,gmp"
    },
    "lib-mpfr": {
      "local": "yes",
      "name": "MPFR library",
      "sort": 20,
      "tags": "lib,mpfr"
    }
  },
  "soft_name": "MPC library",
  "tags": [
    "lib",
    "mpc"
  ]
}
