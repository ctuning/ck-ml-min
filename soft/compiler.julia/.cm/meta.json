{
  "auto_detect": "yes",
  "customize": {
    "add_only_with_version": "yes",
    "add_to_path": "yes",
    "build_dir_name": "julia",
    "check_that_exists": "yes",
    "ck_version": 10,
    "env_prefix": "CK_ENV_COMPILER_JULIA",
    "languages": [
      "JULIA"
    ],
    "limit_recursion_dir_search": {
      "linux": 3,
      "win": 4
    },
    "only_for_target_os_tags": [
      "windows",
      "linux"
    ],
    "soft_file": {
      "linux": "julia",
      "win": "julia.exe"
    },
    "soft_file_from_host_os": "yes",
    "soft_path_example": {
      "linux": "/usr/bin/julia",
      "win": "C:\\Program Files\\Julia\\julia.exe"
    },
    "soft_version_cmd": {
      "linux": "--version > $#filename#$ 2>&1",
      "win": "--version > $#filename#$ 2>&1"
    }
  },
  "env": {
    "CK_COMPILER_FLAGS_OBLIGATORY": "",
    "CK_JULIA_RT": "julia"
  },
  "soft_name": "julia compiler",
  "tags": [
    "compiler",
    "julia",
    "lang-julia"
  ]
}
