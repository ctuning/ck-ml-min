{
  "auto_detect": "yes",
  "customize": {
    "check_that_exists": "yes",
    "ck_version": 10,
    "env_prefix": "CK_ENV_LIB_QT",
    "limit_recursion_dir_search": {
      "linux": 5,
      "win": 5
    },
    "soft_file": {
      "linux": "qmake",
      "win": "qmake.exe"
    },
    "soft_path_example": {
      "linux": "/usr/lib/x86_64-linux-gnu/qt5/bin/qmake",
      "win": "C:\\Qt\\5.8\\msvc2015_64\\bin\\qmake.exe"
    },
    "soft_version_cmd": {
      "linux": "--version > $#filename#$ 2>&1",
      "win": " --version > $#filename#$ 2>&1"
    }
  },
  "soft_name": "Qt library",
  "tags": [
    "lib",
    "qt"
  ]
}
