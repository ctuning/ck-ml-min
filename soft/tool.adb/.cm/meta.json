{
  "auto_detect": "yes",
  "customize": {
    "add_only_with_version": "yes",
    "add_to_path": "yes",
    "build_dir_name": "adb",
    "check_that_exists": "yes",
    "ck_version": 10,
    "env_prefix": "CK_ENV_TOOL_ADB",
    "limit_recursion_dir_search": {
      "linux": 4,
      "win": 5
    },
    "only_for_host_os_tags": [
      "windows",
      "linux"
    ],
    "soft_file": {
      "linux": "adb",
      "win": "adb.exe"
    },
    "soft_file_from_host_os": "yes",
    "soft_path_example": {
      "linux": "/usr/bin/adb",
      "win": ""
    },
    "soft_version_cmd": {
      "linux": "--version > $#filename#$ 2>&1",
      "win": "--version > $#filename#$ 2>&1"
    }
  },
  "env": {},
  "soft_name": "adb",
  "tags": [
    "tool",
    "adb"
  ]
}
