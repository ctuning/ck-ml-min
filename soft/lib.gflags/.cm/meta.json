{
  "auto_detect": "yes",
  "customize": {
    "check_that_exists": "yes",
    "ck_version": 10,
    "env_prefix": "CK_ENV_LIB_GFLAGS",
    "limit_recursion_dir_search": {
      "linux": 3,
      "win": 3
    },
    "soft_file": {
      "linux": "libgflags$#file_ext_dll#$",
      "win": "gflags.lib"
    },
    "soft_path_example": {
      "linux": "/usr/lib/x86_64-linux-gnu/libgflags.so",
      "win": ""
    }
  },
  "deps": {},
  "soft_name": "GFlags library",
  "tags": [
    "lib",
    "gflags"
  ]
}
