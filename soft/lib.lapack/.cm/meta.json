{
  "auto_detect": "yes",
  "customize": {
    "add_include_path": "yes",
    "check_that_exists": "yes",
    "ck_version": 10,
    "env_prefix": "CK_ENV_LIB_LAPACK",
    "limit_recursion_dir_search": {
      "linux": 4,
      "win": 4
    },
    "soft_file": {
      "linux": "liblapack$#file_ext_dll#$",
      "win": "liblapack.lib"
    }
  },
  "soft_name": "LAPACK library",
  "tags": [
    "lib",
    "lapack"
  ]
}
