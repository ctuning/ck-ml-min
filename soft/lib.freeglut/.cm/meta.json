{
  "auto_detect": "yes",
  "customize": {
    "check_that_exists": "yes",
    "ck_version": 10,
    "env_prefix": "CK_ENV_LIB_FREEGLUT",
    "limit_recursion_dir_search": {
      "linux": 3,
      "win": 4
    },
    "only_for_target_os_tags": [
      "linux",
      "windows"
    ],
    "soft_file": {
      "linux": "libfreeglut.so",
      "win": "freeglut.lib"
    },
    "soft_path_example": {
      "linux": "",
      "win": ""
    }
  },
  "soft_name": "FreeGLUT library",
  "tags": [
    "lib",
    "freeglut"
  ]
}
