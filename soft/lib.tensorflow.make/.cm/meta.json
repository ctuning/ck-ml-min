{
  "auto_detect": "yes",
  "customize": {
    "check_that_exists": "yes",
    "ck_version": 10,
    "env_prefix": "CK_ENV_LIB_TF",
    "limit_recursion_dir_search": {
      "linux": 4,
      "win": 4
    },
    "soft_file": {
      "android": "tensorflow/contrib/makefile/gen/lib/libtensorflow-core.a",
      "linux": "tensorflow/contrib/makefile/gen/lib/libtensorflow-core.a",
      "win": "tensorflow\\contrib\\makefile\\gen\\lib\\libtensorflow-core.a"
    }
  },
  "soft_name": "TensorFlow library (via make)",
  "tags": [
    "lib",
    "tensorflow-make"
  ]
}
