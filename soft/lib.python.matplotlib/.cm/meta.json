{
  "auto_detect": "yes",
  "customize": {
    "check_that_exists": "yes",
    "env_prefix": "CK_ENV_PYTHON_PACKAGE_MATPLOTLIB",
    "limit_recursion_dir_search": {
      "linux": 5,
      "win": 5
    },
    "soft_file_universal": "matplotlib$#sep#$__init__.py",
    "version_module_name": "_version",
    "version_variable_name": "version_json"
  },
  "soft_name": "Python Matplotlib library",
  "tags": [
    "lib",
    "python-package",
    "matplotlib"
  ]
}
