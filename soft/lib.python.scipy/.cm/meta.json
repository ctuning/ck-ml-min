{
  "auto_detect": "yes",
  "customize": {
    "check_that_exists": "yes",
    "env_prefix": "CK_ENV_PYTHON_PACKAGE_SCIPY",
    "limit_recursion_dir_search": {
      "linux": 5,
      "win": 5
    },
    "soft_file_universal": "scipy$#sep#$__init__.py",
    "version_module_name": "version",
    "version_variable_name": "version"
  },
  "soft_name": "Python SciPy library",
  "tags": [
    "lib",
    "python-package",
    "scipy"
  ]
}
