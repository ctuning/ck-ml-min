{
  "customize": {
    "add_only_with_version": "yes",
    "add_to_path": "yes",
    "build_dir_name": "php",
    "check_that_exists": "yes",
    "ck_version": 10,
    "env_prefix": "CK_ENV_COMPILER_PHP",
    "languages": [
      "PHP"
    ],
    "limit_recursion_dir_search": {
      "linux": 3,
      "win": 4
    },
    "only_for_host_os_tags": [
      "windows",
      "linux"
    ],
    "only_for_target_os_tags": [
      "windows",
      "linux"
    ],
    "soft_file": {
      "linux": "php",
      "win": "php.exe"
    },
    "soft_file_from_host_os": "yes",
    "soft_version_cmd": {
      "linux": "--version > $#filename#$ 2>&1",
      "win": "--version > $#filename#$ 2>&1"
    }
  },
  "env": {
    "CK_PC": "php",
    "CK_PHP_RT": "php"
  },
  "soft_name": "PHP compiler",
  "tags": [
    "compiler",
    "php",
    "lang-php"
  ]
}
